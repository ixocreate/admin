{
    "name": "ixocreate/admin",
    "description": "IXOCREATE Admin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "dflydev/fig-cookies": "^1.0",
        "firebase/php-jwt": "^5.0",
        "ixocreate/admin-frontend": "dev-develop",
        "ixocreate/application-http": "dev-develop",
        "ixocreate/asset": "dev-develop",
        "ixocreate/command-bus": "dev-develop",
        "ixocreate/common-types": "dev-develop",
        "ixocreate/config": "dev-develop",
        "ixocreate/database": "dev-develop",
        "ixocreate/entity": "dev-develop",
        "ixocreate/event": "dev-develop",
        "ixocreate/intl": "dev-develop",
        "ixocreate/resource": "dev-develop",
        "ixocreate/schema": "dev-develop",
        "ixocreate/servicemanager": "dev-develop",
        "ixocreate/template": "dev-develop",
        "ramsey/uuid": "^3.7",
        "yzalis/identicon": "^1.2"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "ixocreate/media": "dev-develop",
        "php-coveralls/php-coveralls": "^2.0",
        "phpunit/phpunit": "^6.0.6"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateTest\\Admin\\": "tests/src/",
            "IxocreateMisc\\Admin\\": "tests/misc/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
