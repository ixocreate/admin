{
    "name": "ixocreate/admin-package",
    "description": "IXOCREATE Admin Package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "dflydev/fig-cookies": "^1.0",
        "firebase/php-jwt": "^5.0",
        "ixocreate/application": "^0.1",
        "ixocreate/admin-frontend": "^0.1",
        "ixocreate/command-bus-package": "^0.2",
        "ixocreate/database-package": "^0.1",
        "ixocreate/event-package": "^0.1",
        "ixocreate/filter-package": "^0.1",
        "ixocreate/intl-package": "^0.1",
        "ixocreate/resource-package": "^0.1",
        "ixocreate/template-package": "^0.1",
        "ramsey/uuid": "^3.7",
        "yzalis/identicon": "^1.2"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^2.0",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ixocreate\\Test\\Admin\\": "tests/src/",
            "Ixocreate\\Misc\\Admin\\": "tests/misc/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
